<config>
	<remove xpath="/spawning/biome[@name='pine_forest']"/>
	<remove xpath="/spawning/biome[@name='desert']"/>
	<remove xpath="/spawning/biome[@name='snow']"/>
	<remove xpath="/spawning/biome[@name='burnt_forest']"/>	
	<remove xpath="/spawning/biome[@name='wasteland']"/>	
	
	<append xpath="/spawning">
		<biome name="water">
		  <spawn maxcount="10"  time="Any" entitygroup="WaterSpawns" />
		</biome>
		
		<biome name="pine_forest">
		
			<spawn maxcount="3" respawndelay="7" time="Any" entitygroup="ZombiesAll" notags="commercial,industrial,downtown" />
			<spawn maxcount="1" respawndelay="7" time="Night" entitygroup="ZombiesNight" notags="commercial,industrial,downtown" />

			<spawn maxcount="6" respawndelay="7" time="Any" entitygroup="ZombiesAll" tags="commercial,industrial" notags="downtown" />
			<spawn maxcount="2" respawndelay="7" time="Night" entitygroup="ZombiesNight" tags="commercial,industrial" notags="downtown" />

			<spawn maxcount="12" respawndelay="7" time="Any" entitygroup="ZombiesForestDowntown" tags="downtown" />
			<spawn maxcount="4" respawndelay="7" time="Night" entitygroup="ZombiesForestDowntownNight" tags="downtown" />

			<spawn maxcount="1" respawndelay="1" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
			<spawn maxcount="1" respawndelay="1.1" time="Night" entitygroup="EnemyAnimalsForest" spawnDeadChance="0" />	
			
			<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="OblivionCrateAOO" spawnDeadChance="0" />		
		
			<spawn maxcount="1" respawndelay="1" time="Day" entitygroup="FriendlyAnimalsForest" spawnDeadChance="0" /> 
			
		</biome>
		

		<biome name="desert">
			
			<spawn maxcount="3" respawndelay="7" time="Any" entitygroup="ZombiesAll" notags="commercial,industrial,downtown" />
			<spawn maxcount="1" respawndelay="7" time="Night" entitygroup="ZombiesNight" notags="commercial,industrial,downtown" />
			
			<spawn maxcount="12" respawndelay="7" time="Any" entitygroup="ZombiesAll" tags="commercial,industrial" notags="downtown" />
			<spawn maxcount="4" respawndelay="7" time="Night" entitygroup="ZombiesNight" tags="commercial,industrial" notags="downtown" />

			<spawn maxcount="24" respawndelay="7" time="Any" entitygroup="ZombiesDowntown" tags="downtown" />
			<spawn maxcount="8" respawndelay="7" time="Night" entitygroup="ZombiesDowntown" tags="downtown" />
			
			<spawn maxcount="1" respawndelay="1" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
			<spawn maxcount="1" respawndelay="3.5" time="Any" entitygroup="EnemyAnimalsDesert" spawnDeadChance="0" />
		
			<spawn maxcount="1" respawndelay="2" time="Any" entitygroup="FriendlyAnimalsDesert" spawnDeadChance="0" />  

			<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="OblivionCrateAOO" spawnDeadChance="0" />

		</biome>
		
		<biome name="snow">
		
			<spawn maxcount="3" respawndelay="7" time="Any" entitygroup="SnowZombies" notags="downtown" />
			<spawn maxcount="1" respawndelay="7" time="Night" entitygroup="ZombiesNight" notags="commercial,industrial,downtown" />

			<spawn maxcount="12" respawndelay="7" time="Any" entitygroup="ZombiesAll" tags="commercial,industrial" notags="downtown" />
			<spawn maxcount="4" respawndelay="7" time="Night" entitygroup="ZombiesNight" tags="commercial,industrial" notags="downtown" />

			<spawn maxcount="24" respawndelay="7" time="Any" entitygroup="ZombiesDowntown" tags="downtown" />
			<spawn maxcount="8" respawndelay="7" time="Night" entitygroup="ZombiesDowntown" tags="downtown" />

			<spawn maxcount="1" respawndelay="1" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
			<spawn maxcount="1" respawndelay="3.1" time="Any" entitygroup="EnemyAnimalsSnow" spawnDeadChance="0" />		

			<spawn maxcount="1" respawndelay="3" time="Any" entitygroup="FriendlyAnimalsSnow" spawnDeadChance="0" />

			<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="OblivionCrateAOO" spawnDeadChance="0" />
			
		</biome> 
		
		<biome name="burnt_forest">
		
			<spawn maxcount="3" respawndelay="7" time="Any"	entitygroup="ZombiesWasteland" notags="commercial,industrial,downtown" />
			<spawn maxcount="1" respawndelay="7" time="Night" entitygroup="ZombiesWastelandNight" notags="commercial,industrial,downtown" />

			<spawn maxcount="12" respawndelay="7" time="Any" entitygroup="ZombiesWasteland" tags="commercial,industrial" notags="downtown" />
			<spawn maxcount="4" respawndelay="7" time="Night" entitygroup="ZombiesWastelandNight" tags="commercial,industrial" notags="downtown" />

			<spawn maxcount="24" respawndelay="7" time="Any" entitygroup="ZombiesWastelandDowntown" tags="downtown" />
			<spawn maxcount="8" respawndelay="7" time="Night" entitygroup="ZombiesWastelandDowntown" tags="downtown" />

			<spawn maxcount="2" respawndelay="0.3" time="Any" entitygroup="EnemyAnimalsWasteland" spawnDeadChance="0" />
			<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="OblivionCrateAOO" spawnDeadChance="0" />
			
		</biome>

		<biome name="wasteland">
		
			<spawn maxcount="6" respawndelay="7" time="Any"	entitygroup="ZombiesWasteland" notags="commercial,industrial,downtown" />
			<spawn maxcount="3" respawndelay="7" time="Night" entitygroup="ZombiesWastelandNight" notags="commercial,industrial,downtown" />

			<spawn maxcount="24" respawndelay="7" time="Any" entitygroup="ZombiesWasteland" tags="commercial,industrial" notags="downtown" />
			<spawn maxcount="12" respawndelay="7" time="Night" entitygroup="ZombiesWastelandNight" tags="commercial,industrial" notags="downtown" />

			<spawn maxcount="48" respawndelay="7" time="Any" entitygroup="ZombiesWastelandDowntown" tags="downtown" />
			<spawn maxcount="32" respawndelay="7" time="Night" entitygroup="ZombiesWastelandDowntown" tags="downtown" />

			<spawn maxcount="4" respawndelay="0.3" time="Any" entitygroup="EnemyAnimalsWasteland" spawnDeadChance="0" />

			<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="OblivionCrateAOO" spawnDeadChance="0" />
			
		</biome>
		
	</append>

</config>
